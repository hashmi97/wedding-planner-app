# Frontend - used by Vite (VITE_* are exposed to client)
VITE_ADMIN_TOKEN=your-secret-token

# Site password - if set, visitors must enter this to view the app (leave empty to disable)
VITE_APP_PASSWORD=

# Couple names - used for task assignees (defaults: Bride, Groom)
VITE_BRIDE_NAME=
VITE_GROOM_NAME=

# Backend - used by Netlify Functions only
ADMIN_TOKEN=your-secret-token
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
